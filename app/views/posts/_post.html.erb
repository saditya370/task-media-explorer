<div class= "my-3 mb-3" id="<%= dom_id post %>">
  <p>
    
    
    <%if post.photo.attached?%>
        <%=image_tag post.photo , width: '200px' ,class: " max-w-full h-auto border-1 border-gray-200 rounded p-1 selector"%>

    <%end%>
  </p>

</div>

<style>
.selector {
    user-drag: none;
    -webkit-user-drag: none;
    user-select: none;
    -moz-user-select: none;
    -webkit-user-select: none;
    -ms-user-select: none;
}
</style>
